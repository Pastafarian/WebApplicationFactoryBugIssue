{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "Hostnames": [ "127.0.0.1" ],
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "VHost": "/",
    "LoggingExchangeName": "log-exchange",
    "ExchangeType": "fanout",
    "LoggingQueueName": "log-queue",
    "BatchPostingLimit": 2,
    "Period": "00:00:00:02",
    "AutoCreateExchange": true,
    "MinimumLevel": "Debug"
  }
}
